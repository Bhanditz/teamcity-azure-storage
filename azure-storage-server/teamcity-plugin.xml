<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>azure-storage</name>
        <display-name>Azure Artifact Storage</display-name>
        <version>@Plugin_Version@</version>
        <description>Allows storing build artifacts in an Azure storage account</description>
        <download-url>https://github.com/JetBrains/teamcity-azure-storage</download-url>
        <vendor>
            <name>JetBrains, s.r.o.</name>
            <url>http://www.jetbrains.com/</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true"/>
    <requirements min-build="46654" />
</teamcity-plugin>
